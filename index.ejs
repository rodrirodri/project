<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>My CRUD in NodeJS</title>
</head>

<body>
    <h2>Registrar<h2>
    <form action="/quotes" method="POST">
        <input type="text" placeholder="nome" name="nome">
        <input type="text" placeholder="placa" name="placa">
        <input type="text" placeholder="cpf" name="cpf">
        <button type="submit">Entrar</button>
    </form>
    
   

    <h2> Quotes </h2>

<ul class="quotes">
  <!-- Loop through quotes -->
  <% for(var i = 0; i < quotes.length; i++) {%>
    <li class="quote">
      <!-- Output name from the iterated quote object -->
      <span><%= quotes[i].nome %></span>:
      <!-- Output quote from the iterated quote object -->
      <span><%= quotes[i].placa %></span>
      <span><%= quotes[i].cpf %></span>
    </li>
  <% } %>
</ul>

</body>
</html>